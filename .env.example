# ============================================
# ENVIRONMENT VARIABLES EXAMPLE
# Copy file ini ke .env.development dan isi nilai yang sesuai
# ============================================

# App Information
NUXT_PUBLIC_APP_NAME=Sistem Koperasi Indonesia
NUXT_PUBLIC_APP_VERSION=1.0.0

# API Configuration
NUXT_PUBLIC_API_BASE_URL=http://localhost:3002
NUXT_PUBLIC_API_TIMEOUT=30000

# App URL
NUXT_PUBLIC_APP_URL=http://sistemkoperasi.local:3000

# Domain Configuration
NUXT_PUBLIC_MAIN_DOMAIN=sistemkoperasi.local
NUXT_PUBLIC_USE_HTTPS=false

# Multi-tenant Mode
NUXT_PUBLIC_TENANT_MODE=subdomain

# Debug Mode
NUXT_PUBLIC_DEBUG_MODE=true