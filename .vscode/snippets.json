{
  "Vue SFC": {
    "prefix": "vue",
    "body": [
      "<script setup lang=\"ts\">",
      "$1",
      "</script>",
      "",
      "<template>",
      "  <div>",
      "    $2",
      "  </div>",
      "</template>"
    ],
    "description": "Vue SFC with script setup"
  },
  "Nuxt Composable": {
    "prefix": "composable",
    "body": [
      "export const use${1:Name} = () => {",
      "  $2",
      "  return { $3 }",
      "}"
    ],
    "description": "Nuxt auto-importable composable"
  }
}
